var jdex = require("./js/jdex.js");
var fs = require('fs');
var $ = require("jquery");
var DOMParser = require('xmldom').DOMParser;

var xml_text = fs.readFileSync('./xbel_test.xbel.xml').toString();

var graph = jdex.createGraphFromXBEL(xml_text);

var jdex_graph_string = graph.serializeJDEx();

var url = 'localhost:9999/networks/';

  /* Post the data via Ajax and process the returned JSON*/
$.ajax({
	   type: "POST",
	   url: url,
	   data: jdex_graph_string;
	   dataType: "JSON",
	   success: function(data)
	   {
		   alert("OK : " + JSON.stringify(data)); // show response
	   },
	   error: function(data){
			alert("Error : " + JSON.stringify(data));
	   }
});

console.log("done");